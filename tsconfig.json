{
  "compilerOptions": {
    "module": "umd",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": false,
    "moduleResolution": "Node"
  },
  "exclude": [
    "node_modules",
    "bower_components",
    "dist",
    "source"
  ]
}
